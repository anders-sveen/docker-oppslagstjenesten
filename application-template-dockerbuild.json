{
  "apiVersion": "v1beta1",
  "items": [
    {
      "apiVersion": "v1beta1",
      "containerPort": 8080,
      "id": "oppslagstjenesten",
      "kind": "Service",
      "port": 8080,
      "selector": {
        "name": "oppslagstjenesten"
      }
    },
    {
      "apiVersion": "v1beta1",
      "kind": "ImageRepository",
      "metadata": {
        "name": "oppslagstjenesten"
      }
    },
    {
      "apiVersion": "v1beta1",
      "kind": "ImageRepository",
      "metadata": {
        "name": "java"
      },
      "dockerImageRepository": "java"
    },
    {
      "apiVersion": "v1beta1",
      "kind": "BuildConfig",
      "metadata": {
        "name": "oppslagstjenesten-build",
        "labels": {
          "name": "oppslagstjenesten-build"
        }
      },
      "parameters": {
        "output": {
          "to": {
            "name": "oppslagstjenesten"
          }
        },
        "source": {
          "git": {
            "uri": "https://github.com/Skatteetaten/docker-oppslagstjenesten.git"
          },
          "type": "Git"
        },
        "strategy": {
          "type": "Docker"
        }
      }

      "triggers": [
        {
          "imageChange": {
            "from": {
              "name": "java"
            },
            "image": "java",
            "tag": "latest"
          },
          "type": "imageChange"
        }
      ]

    },
    {
      "apiVersion": "v1beta1",
      "kind": "DeploymentConfig",
      "metadata": {
        "name": "oppslagstjenesten"
      },
      "template": {
        "controllerTemplate": {
          "podTemplate": {
            "desiredState": {
              "manifest": {
                "containers": [
                  {
                    "env": [
                      {
                        "name": "PORT_BASE",
                        "value": "${PORT_BASE}"
                      }
                    ],
                    "image": "oppslagstjenesten",
                    "name": "oppslagstjenesten",
                    "ports": [
                      {
                        "containerPort": 8080
                      }
                    ]
                  }
                ],
                "version": "v1beta1"
              }
            },
            "labels": {
              "name": "oppslagstjenesten"
            }
          },
          "replicaSelector": {
            "name": "oppslagstjenesten"
          },
          "replicas": 1
        },
        "strategy": {
          "type": "Recreate"
        }
      },
      "triggers": [
        {
          "imageChangeParams": {
            "automatic": true,
            "containerNames": [
              "oppslagstjenesten"
            ],
            "from": {
              "name": "oppslagstjenesten"
            },
            "tag": "latest"
          },
          "type": "ImageChange"
        }
      ]
    }
  ],
  "kind": "Template",
  "metadata": {
    "annotations": {
      "description": "This example shows how to create a simple java application in openshift origin v3"
    },
    "name": "oppslagstjenesten"
  },
  "parameters": [
    {
      "description": "Listening port for oppslagstjenesten",
      "name": "PORT_BASE",
      "value": "8080"
    }
  ],
  "labels": {
    "template": "application-template-dockerbuild"
  }
}
